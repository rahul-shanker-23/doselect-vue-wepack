{
    "version": "1.0",
    "default_open_files": [
      ],
    "read_only_files": [
      "tests/*", 
      ".gitignore", 
      "configuration.json"
      ],
    "buttons": {
      "Install Packages": {
        "command": "npm install"
      },
      "Start Dev Server": {
        "command": "npm start"
      },
      "Start API Server":{
        "command": "npm run server"
      },
      "Run Test Cases": {
        "command": "npm test -- --watchAll=false"
      }
    },
    "scoring": {
      "command": "npm install && npm run test -- --testResultsProcessor=\"jest-junit\" --watchAll=false",
      "files": {
        "junit.xml": "Vue"
      }
    },
    "post_start": {
      "commands": ["npm install && npm run server"]
    },
    "preview_port": 3000
  }
  